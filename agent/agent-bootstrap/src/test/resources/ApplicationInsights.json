{
  "instrumentationSettings": {
    "connectionString": "InstrumentationKey=00000000-0000-0000-0000-000000000000",
    "preview": {
      "roleName": "Something Good",
      "roleInstance": "xyz123",
      "jmxMetrics": [
        {
          "objectName": "java.lang:type=Threading",
          "attribute": "ThreadCount",
          "display": "Thread Count"
        },
        {
          "objectName": "java.lang:type=Memory",
          "attribute": "HeapMemoryUsage.init",
          "display": "Initial Heap Memory"
        },
        {
          "objectName": "java.lang:type=ClassLoading",
          "attribute": "LoadedClassCount",
          "display": "Current Class Loaded Count"
        }
      ],
      "sampling": {
        "fixedRate": {
          "percentage": 10
        },
        "adaptiveRate": {
          "default": {
            "initialRate": 10,
            "maxPerInterval": 50,
            "intervalInSeconds": 1
          },
          "requests": {
            "initialRate": 10,
            "maxPerInterval": 50,
            "intervalInSeconds": 1
          }
        }
      },
      "distributedTracing": {
        "outboundEnabled": false,
        "requestIdCompatEnabled": false
      },
      "liveMetrics": {
        "enabled": false
      },
      "instrumentation": {
        "jdbc": {
          "__comment": [
            "this sets the explain plan threshold ...",
            "this is a multi-line comment"
          ],
          "explainPlanThresholdInMS": 20000
        },
        "logging": {
          "enabled": false
        }
      },
      "customInstrumentation": [
        {
          "className": "com.example.MyObject",
          "methodName": "doSomething"
        },
        {
          "className": "com.example.MyObject",
          "methodName": "doMoreSomething"
        }
      ]
    }
  }
}
