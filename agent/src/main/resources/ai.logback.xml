<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration>
<configuration>
  <appender name="CONSOLE" class="com.microsoft.applicationinsights.agent.shadow.ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>
  <logger name="org.glowroot.instrumentation" level="info" />
  <logger name="com.microsoft.applicationinsights.agent.shadow" level="warn" />
  <root level="info">
    <appender-ref ref="CONSOLE" />
  </root>
</configuration>
